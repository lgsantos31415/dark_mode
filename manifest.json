{
  "manifest_version": 3,
  "name": "Dark Mode",
  "description": "Uma extens√£o criada para aplicar modo escuro no site 'buildyourownlisp'.",
  "version": "0.1",
  "permissions": ["tabs", "scripting"],
  "background": {
    "service_worker": "./background.js"
  },
  "host_permissions": ["https://www.buildyourownlisp.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icons/16.png",
      "48": "./icons/48.png",
      "128": "./icons/128.png"
    }
  },
  "icons": {
    "16": "./icons/16.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  }
}
